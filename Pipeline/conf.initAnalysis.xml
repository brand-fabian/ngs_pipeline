	<windows>
		<SureSelect38Mb>50</SureSelect38Mb>
		<SureSelect50Mb>50</SureSelect50Mb>
		<SureSelectMouse50Mb>50</SureSelectMouse50Mb>
		<SureSelect50Mbv4>50</SureSelect50Mbv4>
		<SureSelect50Mbv5>50</SureSelect50Mbv5>
		<SureSelect60Mbv6>50</SureSelect60Mbv6>
		<genomic>50</genomic>
		<MIP_RLS>0</MIP_RLS>
		<MIP_CAD>0</MIP_CAD>
		<genomicmouse>50</genomicmouse>
		<Nextera62Mb>50</Nextera62Mb>
		<TruSightOneV1>50</TruSightOneV1>
		<SeqCapEZExomeV2>50</SeqCapEZExomeV2>
		<SeqCapEZExomeV3>50</SeqCapEZExomeV3>
		<NexteraRapidCaptureV1.2>50</NexteraRapidCaptureV1.2>
		<TruSeqExome62Mb>50</TruSeqExome62Mb>
		<ACE3_Personalis>50</ACE3_Personalis>
		<SureSelect75Mbv5UTR>50</SureSelect75Mbv5UTR>
