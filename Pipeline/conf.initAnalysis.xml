		<SureSelect60Mbv6_200ng_XTLI>50</SureSelect60Mbv6_200ng_XTLI>
		<PERSPECTIVE>50</PERSPECTIVE>
		<SeqCapEZMedExome>50</SeqCapEZMedExome>
	</windows>
	<solexadb>
	<host>SERVER13</host>
	<port>DBPORT</port>
	<database>solexa</database>
	<user>DBUSER</user>
	<password>DBPWD</password>
	</solexadb>
	<dirs>
		<out>out</out>
	</dirs>
	<runs>
		<alignmentOnly>
			<run>alignment:4-6</run>
		</alignmentOnly>
		
		<standard>
			<run>bam_merge</run>
			<run>transcriptStats</run>
			<run>stats</run>
			<run>checkContamination</run>
			<run>parsestats</run>
			<run>statsdb</run>
			<run>insertSizeIntoDB</run>
		</standard>
		<exome>
			<run>varfilter</run>
			<run>runPindel</run>
			<run>filterPindel</run>
			<run>annotatePindel</run>
			<run>exomeDepth</run>
			<run>annotateSNPs</run>
			<run>checkPileup</run>
			<run>transversion</run>
			<run>snpEff</run>
			<run>RefSeq</run>
			<run>lncRNA</run>
			<run>miRNA</run>
			<run>callChrM</run>
			<run>annotateChrM</run>
		</exome>
		
		<exomedb>
			<run>insertPindel</run>
			<run>exomeDepthInsert</run>
			<run>snvdbInsertExome</run>
			<run>snpEffInsert</run>
			<run>homozygosity</run>
			<run>updateRefSeq</run>
			<run>updatelncRNA</run>
			<run>updatemiRNA</run>
			<run>insertChrM</run>		
		</exomedb>
		
		<genome>
			<run>cnvnator</run>
			<run>runBreakdancer</run>
			<run>runLumpy</run>
			<run>runManta:4-6</run>
			<disabled>runBellerophon</disabled>
			<run>runIGVtools</run>
			<run>mergeSV</run>
		</genome>
	
		<genomedb>
			<disabled>insertSV</disabled>
			<disabled>insertBellerophon</disabled>
			<run>genomerun</run>
			<run>genomerun</run>
		</genomedb>
	
		<rnaseq>
			<run>htseqcount</run>
			<run>calcFPKM</run>
			<run>normalization</run>
			<run>RNAdbInsert</run>
			<run>calcRNAstats:4</run>
			<run>RNAStatsDBInsert</run>
		</rnaseq>
		
		<chipseq>
			<run>runMacs</run>
		</chipseq>
		
		<mtDNA>
			<run>runPindel</run>
			<run>filterPindel</run>
			<run>annotatePindel</run>
			<run>insertPindel</run>
			<run>callChrM</run>
			<run>annotateChrM</run>
			<run>insertChrM</run>
		</mtDNA>
		
	</runs>
	<scripts>
		<parseConfig>pipelinecfg_gatk.pl -i</parseConfig>
		<sequence2BAMpl>copyRunFolderFiles.pl</sequence2BAMpl></scripts></config>
